{
  "conventionalCommits.scopes": [
    "repo",
    "dev",
    "build",
    "lint",
    "publish",
    "ci",
    "git",
    "deps",
    "changelog",
    "readme",
    "package"
  ],

  "prettier.enable": false,
  "editor.codeActionsOnSave": {
    "source.addMissingImports": true,
    "source.fixAll.eslint": true
  },
}
